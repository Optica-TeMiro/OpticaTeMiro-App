<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/buttonColor"
    tools:context=".ui.product.ProductServicesPpalFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView1"
            android:name="org.mywire.temiroapp.ui.main.TopMenuFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            tools:layout="@layout/main_frag_top_menu" />

        <Button
            android:id="@+id/btnIrAProducto"
            style="@style/botonesMenu"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="222dp"
            android:layout_marginEnd="46dp"
            android:text="Productos"
            />

        <Button
            android:id="@+id/btn2"
            style="@style/botonesMenu"
            android:layout_below="@+id/btnIrAProducto"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="46dp"
            android:text="Servicios" />

    </RelativeLayout>

</FrameLayout>
